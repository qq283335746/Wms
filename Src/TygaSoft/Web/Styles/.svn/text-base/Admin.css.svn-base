/*商品、图片横向展现*/
.row_col {
 height:100% auto; float:left; margin-right:10px; border:solid 1px #dddddd; padding:10px; margin-bottom:10px !important;padding-right: 0;
}
.row_col .img{
 width:100px;height:100px;
}
.curr {
 border:solid 1px #FF0000 !important;
}
.title h1 {
 text-align:center;font-size:16px;
}

/*header*/

#header .aa {
 background:url('Images/bg_lightgreen.png'); height:100% auto; color:#FFFFFF;
:}
#header .aa .aaa {
 font-weight: 700;font-size:24px; text-align:center;  line-height: 62px;
}
#header .aa .aab {
 font-weight:600; text-align:center;font-size:18px; height:30px; line-height:30px; width:310px; float:right;
}
#header .aa .aac {
 font-size:12px; width:342px; float:left; text-align:right; line-height:41px;
}
#header a {
 color:#FFFFFF; 
}
#header .lvUser {
 position: absolute;top: 20px;right: 20px;
}

/*footer*/
#footer .aa {
 text-align:center;
}

/*barcodeCtr*/
.sw-barcode table {
 width:100%;margin:0 auto;border-spacing:0;border-collapse:collapse;
}
.sw-barcode .border td{
 text-align:center;border:1px solid #000;overflow:hidden;
}
.sw-barcode .border td.curr-merge{
 background:#e6e6e6;color:#00438a;
}

/*easyui*/
.combobox-item{
 line-height: 16px;
}
.validatebox-invalid {
 border-color:#bbb;background-color: #fff;
}
.textbox-invalid {
 border-color:#bbb;background-color: #fff;
}
.easyui-tree .tree-expanded {
 background-position:-112px 0;
}
.easyui-tree .tree-collapsed {
 background-position:-97px 0;
}
.easyui-tree .tree-file {
 
}
.easyui-tree .tree-folder {
 background-image:none;width:0;
}
.easyui-tree .tree-node {
 
}
#westTree .tree-indent {
 display:none;
}

#westTree .tree-node {
 padding:8px 10px; background:#F5CCAE; color:#562812;
}

.datagrid-cell{
     white-space:normal;word-wrap: break-word;word-break: break-all;
}

/*菜单*/
#west .accordion-body li{
 padding:3px 0;
}
#west .accordion-body li a{
 padding:5px;font-size: 14px;
}
.menus .tree-file {
 display:none;
}
.menus .tree-node {
 font-size: 14px;padding-top:2px;padding-bottom:2px;
}
.acc .menus .tree-expanded {
 margin-left: 15px;
}
.acc .menus .tree-collapsed {
 margin-left: 15px;
}
.acc .menus .tree-node-selected {
 background-color:#fff;
}
.acc .menus .tree-node-selected .tree-title{
 color: #008800;font-size: 16px;
}

.infoT {
 width:100%;
}
.infoT tr td {
 padding:3px; vertical-align:top;
}
.infoT .tar {
 text-align:right; width:100%;
}
.infoT .txt {
 border: 1px solid #BBBBBB;height: 14px;padding: 4px 3px;width: 120px; background:#FFFFFF;
}

div.h_r {
 height:30px; line-height:30px;overflow:hidden;
}
div.h_r .ddl_r {
 height:22px;width:88px;
}
.datagrid-row-selected .abtn,.datagrid-row-selected .c-g,.datagrid-row-selected .cy {
 color:#FFFF00;
}
.row .textbox, .row-fl .textbox{
display:block;
}